library(googlesheets4)
datos <- read_sheet('https://docs.google.com/spreadsheets/d/1FW4EtmIWLB5bo0mXUDiOGJeI9g6uCTxEM00clT2V9ug/edit?usp=sharing')
# table(datos$`¿Cuál es la identidad de género de la persona encuestada?`)
# datos = subset(datos, datos$`¿Cuál es la identidad de género de la persona encuestada?` == "varón")
library(tidyverse)
datos = datos %>% mutate_if(is.list,as.numeric)
summary(datos)

table(datos$orden_inflacion)
table(datos$orden_inseguridad)
table(datos$orden_desempleo)
table(datos$orden_pobreza)
table(datos$orden_deficit)
table(datos$orden_corrupcion)
table(datos$orden_educacion)


